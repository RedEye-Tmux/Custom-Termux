#!/data/data/com.termux/files/usr/bin/bash

#colors
	export negro='\033[1;30m'
        export red='\033[1;31m'
        export green='\033[1;32m'
        export yellow='\033[1;33m'
        export blue='\033[1;34m'
        export magenta='\033[1;35m'
        export cyan='\033[1;36m'
        export reset='\033[0m'

#Banner
printf "$red"
echo   "==================================="
printf "$cyan"
echo   ' ____          _ _____           '
echo   '|  _ \ ___  __| | ____|   _  ___ '
echo   '| |_) / _ \/ _` |  _|| | | |/ _ \'
echo   '|  _ <  __/ (_| | |__| |_| |  __/'
echo   '|_| \_\___|\__,_|_____\__, |\___|'
echo   '                       |___/     '
printf "$red"
echo   "==================================="
printf "$reset"
